<script>
  function ckv(e){
    var v=$(e).val();
    if(v.length > 0){
      $(e).addClass("is-valid")
      $(e).removeClass("is-invalid")
    }else{
      $(e).removeClass("is-valid")
      $(e).addClass("is-invalid")
    }
  }
  </script>
